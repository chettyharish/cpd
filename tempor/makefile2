# an example with inference rules
CC=gcc
CFLAG=-g 
.SUFFIX: .c .o
a.out: myprog1.o myprog2.o myprog3.o 
	$(CC) $(CFLAG) myprog1.o myprog2.o myprog3.o
.c.o:
	$(CC) -c $<
myprog1.o: myprog1.c myprog.h
	gcc -c myprog1.c
	echo "I am target rule"
clean:
	rm -f a.out
	rm -f *.o
	rm -f *~
	rm -f core
demo: a.out
	a.out
